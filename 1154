#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    int n,i;
    double avg,sum=0;
    cout<<fixed<<setprecision(2);
    for(i=1; i<=100; i++)
    {
        cin>>n;
        if(n>0)
        {
           sum=sum+n;
           avg=sum/i;
        }
        else if(n<0)
            break;

    }
    cout<<avg<<endl;
    return 0;
}
