#include<stdio.h>
int main ()
{
    int a,b;
    double total;
    scanf("%d %d",&a,&b);
    if(a==1)
    {
        total = 4.00 * b;
        printf("Total: R$ %.2lf\n",total);
    }
    else if(a==2)
    {
        total = 4.50 * b;
        printf("Total: R$ %.2lf\n",total);
    }
    else if(a==3)
    {
        total = 5.00 * b;
        printf("Total: R$ %.2lf\n",total);
    }
    else if(a==4)
    {
        total = 2.00 * b;
        printf("Total: R$ %.2lf\n",total);
    }
    else if(a==5)
    {
        total = 1.50 * b;
        printf("Total: R$ %.2lf\n",total);
    }
    return 0;
}
