#include<iostream>
using namespace std;
int main()
{
    int i,n,pos=0,neg=0,evn=0,odd=0;
    for(i=1; i<=5; i++)
    {
        cin>>n;
        if(n>0)
            pos++;
        if(n%2==0)
            evn++;
        if(n<0)
            neg++;
        if(n%2!=0)
            odd++;

    }
    cout<<evn<<" valor(es) par(es)"<<endl;
    cout<<odd<<" valor(es) impar(es)"<<endl;
    cout<<pos<<" valor(es) positivo(s)"<<endl;
    cout<<neg<<" valor(es) negativo(s)"<<endl;
    return 0;
}
