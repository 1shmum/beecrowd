#include<iostream>
using namespace std;
int main()
{
    int A,B,C,D;
    cin>>A>>B;
    if(A>B)
    {
        C=((B+24)-A);
        cout<<"O JOGO DUROU "<<C<<" HORA(S)"<<endl;
    }
    else if(A<B)
    {
        D=(B-A);
        cout<<"O JOGO DUROU "<<D<<" HORA(S)"<<endl;
    }
    else if(A==B)
        cout<<"O JOGO DUROU 24 HORA(S)"<<endl;
    return 0;
}
